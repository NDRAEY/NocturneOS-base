{
    "git.repositoryScanIgnoredFolders": [
        "node_modules",
        "rust/target",
        "ramdisk/"
    ],
    "rust-analyzer.diagnostics.styleLints.enable": true,
    "rust-analyzer.linkedProjects": [
        "rust/Cargo.toml",
        "utils/chen/Cargo.toml"
    ],
    "rust-analyzer.restartServerOnConfigChange": true,
    "rust-analyzer.completion.fullFunctionSignatures.enable": true,
    "rust-analyzer.completion.postfix.enable": false,
    "rust-analyzer.completion.privateEditable.enable": false,
    "rust-analyzer.inlayHints.genericParameterHints.type.enable": true,
    "rust-analyzer.inlayHints.genericParameterHints.lifetime.enable": true,
    "rust-analyzer.inlayHints.lifetimeElisionHints.enable": "skip_trivial",
    "files.associations": {
        "sync.h": "c",
        "hexview.h": "c",
        "list.h": "c",
        "stdio.h": "c",
        "icmp.h": "c",
        "ports.h": "c",
        "vmm.h": "c",
        "cpufeature.h": "c",
        "string.h": "c",
        "ata.h": "c",
        "ac97.h": "c",
        "common.h": "c",
        "stack.h": "c",
        "pmm.h": "c",
        "scheduler.h": "c",
        "future": "c",
        "system_error": "c",
        "diskman_commands.h": "c",
        "math.h": "c"
    },
}
